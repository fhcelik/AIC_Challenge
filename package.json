{
  "name": "calcoola",
  "version": "1.24.2",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "jest-enzyme": "^6.0.2",
    "localforage": "^1.7.1",
    "material-ui": "^1.0.0-beta.40",
    "material-ui-chip-input": "^v1.0.0-beta.5",
    "material-ui-icons": "^1.0.0-beta.36",
    "mathjs": "^4.0.1",
    "normalizr": "^3.2.4",
    "prop-types": "^15.5.10",
    "qs": "^6.5.2",
    "ramda": "^0.25.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-katex": "^2.0.1",
    "react-popper": "^0.10.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-stack-grid": "^0.7.1",
    "recompose": "^0.26.0",
    "reduce-reducers": "^0.1.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-debounce": "^1.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^5.9.1",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sinon": "^6.1.4",
    "typeface-oxygen-mono": "^0.0.54",
    "uuid": "^3.2.1",
    "webpack": "3.8.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint **.js",
    "start": "PORT=3005 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "mock-api": "json-server -p 3003 -w api/db.json",
    "storybook": "$npm_execpath run mock-api & start-storybook -p 9001 -c .storybook && fg",
    "start-with-mock-api": "$npm_execpath run mock-api & $npm_execpath start && fg",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@semantic-release/exec": "^2.2.4",
    "@semantic-release/git": "^5.0.0",
    "@storybook/addon-actions": "3.4.2",
    "@storybook/addons": "3.4.2",
    "@storybook/react": "^3.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "dotenv-webpack": "^1.5.7",
    "husky": "^0.14.3",
    "json-server": "^0.12.1",
    "jss-snapshot-serializer": "git://github.com/dev-theory/jss-snapshot-serializer.git",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.4.1",
    "redux-devtools-extension": "^2.13.2",
    "require-context": "^1.1.0",
    "semantic-release": "^15.5.0",
    "server-destroy": "^1.0.1",
    "storybook": "^1.0.0",
    "storybook-react-router": "^1.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.cases.js"
    ]
  }
}
