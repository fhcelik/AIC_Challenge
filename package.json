  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "localforage": "^1.7.1",
    "material-ui": "^1.0.0-beta.40",
    "material-ui-chip-input": "^v1.0.0-beta.5",
    "material-ui-icons": "^1.0.0-beta.36",
    "mathjs": "^4.0.1",
    "normalizr": "^3.2.4",
    "ramda": "^0.25.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-katex": "^2.0.1",
    "react-popper": "^0.10.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "typeface-oxygen-mono": "^0.0.54",
    "uuid": "^3.2.1"
  },
  "lint-staged": {
    "(./**/*.{js, jsx}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint **.js",
    "start": "PORT=3005 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "mock-server": "json-server api/db.json",
    "storybook": "$npm_execpath run mock-server & start-storybook -p 9001 -c .storybook && fg",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@semantic-release/exec": "^2.2.4",
    "@semantic-release/git": "^5.0.0",
    "@storybook/react": "^3.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "husky": "^0.14.3",
    "json-server": "^0.12.1",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1",
    "redux-devtools-extension": "^2.13.2",
    "semantic-release": "^15.5.0",
    "server-destroy": "^1.0.1",
    "storybook": "^1.0.0",
    "storybook-react-router": "^1.0.1"
  }
